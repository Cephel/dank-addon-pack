<Bindings>
	<Binding name="METAMAP_MAPTOGGLE" header="METAMAP_TITLE">
		MetaMap_ToggleFrame(WorldMapFrame);
	</Binding>
	<Binding name="METAMAP_MAPTOGGLE1">
		MetaMap_MapModeToggle(1);
	</Binding>
	<Binding name="METAMAP_MAPTOGGLE2">
		MetaMap_MapModeToggle(2);
	</Binding>
	<Binding name="METAMAP_FSTOGGLE">
		MetaMap_FullScreenToggle();
	</Binding>
	<Binding name="METAMAP_SAVESET">
		MetaMapSaveSet_Toggle();
	</Binding>
	<Binding name = "METAMAP_QUICKNOTE">
		MetaMap_ToggleFrame(MetaMapNotes_QuickNoteFrame);
	</Binding>
	<Binding name = "METAMAP_KB">
		MetaMap_LoadWKB(1)
	</Binding>
	<Binding name = "METAMAP_KB_TARGET_UNIT">
		MetaMap_LoadWKB(2)
	</Binding>
	<Binding name = "METAMAP_QST">
		MetaMap_LoadQST(1)
	</Binding>
</Bindings>
