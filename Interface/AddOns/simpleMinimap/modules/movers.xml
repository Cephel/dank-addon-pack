<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<Frame name="smmMoverTemplate" parent="UIParent" topLevel="true" enableMouse="true" virtual="true">
		<Backdrop bgFile="Interface\Tooltips\UI-Tooltip-Background"/>
	</Frame>

	<Frame name="smmQuestMover" inherits="smmMoverTemplate">
		<Size><AbsDimension x="280" y="80"/></Size>
		<Anchors><Anchor point="CENTER" relativeTo="QuestWatchFrame"/></Anchors>
	</Frame>

	<Frame name="smmDollMover" inherits="smmMoverTemplate">
		<Size><AbsDimension x="60" y="65"/></Size>
		<Anchors><Anchor point="CENTER" relativeTo="DurabilityFrame"/></Anchors>
	</Frame>

	<Frame name="smmTimerMover" inherits="smmMoverTemplate">
		<Size><AbsDimension x="160" y="72"/></Size>
		<Anchors><Anchor point="CENTER" relativeTo="QuestTimerFrame"/></Anchors>
	</Frame>

	<Frame name="smmCaptureMover" inherits="smmMoverTemplate">
		<Size><AbsDimension x="178" y="30"/></Size>
		<Anchors>
			<Anchor point="TOPRIGHT" relativeTo="MinimapCluster" relativePoint="BOTTOMRIGHT">
				<Offset>
					<AbsDimension x="0" y="13"/>
				</Offset>
			</Anchor>
		</Anchors>
	</Frame>

</Ui>
